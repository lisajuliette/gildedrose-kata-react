{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es5",
		"strict": true,
		"noImplicitAny": false,
		"jsx": "react-jsx",
		"jsxImportSource": "@emotion/react",
		"sourceMap": false,
		"baseUrl": "./",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"paths": {
			"@/*": ["src/*"]
		}
	},
	"exclude": ["node_modules"]
}
